{
  "name": "tc39-ci",
  "version": "0.0.7",
  "description": "Publishes TC39 drafts and updates to S3",
  "scripts": {
    "lint": "eslint . --ignore-pattern node_modules --fix",
    "start": "cross-env NODE_ENV=testing npx sandbox",
    "test": "echo 'tests skipped' || cross-env NODE_ENV=testing PORT=6666 tape test/*-test.js ./test/**/*-test.js | faucet",
    "publish": "node tools/publish-preview.js"
  },
  "dependencies": {
    "@architect/functions": "^8.1.1",
    "@begin/data": "^5.0.3",
    "github-api": "^0.10.7",
    "mime-types": "^2.1.35",
    "tar-stream": "^3.1.7"
  },
  "devDependencies": {
    "@architect/architect": "^11.0.12",
    "@ljharb/eslint-config": "^21.1.0",
    "cross-env": "^7.0.3",
    "eslint": "=8.8.0",
    "faucet": "^0.0.4",
    "tape": "^5.7.5",
    "tiny-json-http": "^7.5.1"
  },
  "private": true
}
